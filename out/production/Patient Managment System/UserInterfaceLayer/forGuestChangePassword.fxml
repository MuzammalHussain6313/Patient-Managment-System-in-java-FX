<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="310.0" prefWidth="623.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UserInterfaceLayer.ForGuestChangePasswordController">
    <children>
        <Label layoutX="52.0" layoutY="42.0" prefHeight="30.0" prefWidth="70.0" text="User Name" />
        <TextField fx:id="name" layoutX="176.0" layoutY="36.0" prefHeight="41.0" prefWidth="341.0" promptText="Enter User Name." />
        <Button fx:id="changePasswordButton" layoutX="207.0" layoutY="235.0" mnemonicParsing="false" onAction="#changePassword" prefHeight="41.0" prefWidth="115.0" text="Change Password" />
        <Button fx:id="cancelButton" layoutX="378.0" layoutY="235.0" mnemonicParsing="false" onAction="#cancelChange" prefHeight="41.0" prefWidth="103.0" text="Cancel" />
        <Label layoutX="44.0" layoutY="105.0" prefHeight="30.0" prefWidth="92.0" text="New Password" />
      <PasswordField fx:id="password" layoutX="176.0" layoutY="100.0" prefHeight="41.0" prefWidth="341.0" promptText="Enter Password" />
      <PasswordField fx:id="confirmPassword" layoutX="176.0" layoutY="163.0" prefHeight="41.0" prefWidth="341.0" promptText="Re-enter Password" />
      <Label layoutX="44.0" layoutY="169.0" prefHeight="30.0" prefWidth="103.0" text="Confirm Password" />
    </children>
</AnchorPane>
